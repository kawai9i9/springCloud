<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.weilus.dao.UserDao" >

	  <resultMap type="UserEntity" id="rm_UserEntity">
		<result column="username" property="username"/>
		<result column="userpwd" property="password"/>
		<result column="locked" property="locked"/>
		<result column="enable" property="enable"/>
		  <result column="accountExpired" property="accountExpired"/>
	  </resultMap>

	 <select id="loadUserByUsername" resultMap="rm_UserEntity">
		 SELECT username,userpwd,locked,enable,accountExpired FROM T_USER WHERE USERNAME=#{username}
	 </select>

</mapper>